{{#if page.component}}
<meta name="product" content="{{page.component.title}}">
{{#if page.latest.url}} 
<meta name="version" content="{{or page.displayVersion page.version}}">
{{#with (get-semver page.version)}}
{{#if this.major}}
<meta name="version-major" content="{{this.major}}">
{{/if}}
{{#if this.minor}}
<meta name="version-minor" content="{{this.minor}}">
{{/if}}
{{#if this.patch}}
<meta name="version-patch" content="{{this.patch}}">
{{/if}}
{{/with}}
{{#if (eq page.component.latest.version page.version)}}
<meta name="is-latest-version" content="true">
<meta name="version-with-latest" content="{{or page.displayVersion page.version}} [latest]">
{{else}}
<meta name="latest-version" content="{{page.component.latest.version}}">
<meta name="version-with-latest" content="{{or page.displayVersion page.version}}">
{{/if}}
{{/if}}
<meta name="page-url" content="{{page.url}}">
<meta name="page-component" content="{{page.component.name}}">
<meta name="page-version" content="{{page.version}}">
{{#with site.keys.googleSiteVerification}}
<meta name="google-site-verification" content="{{this}}">
{{/with}}
{{/if}}
{{#if (or antoraVersion site.antoraVersion)}}
<meta name="generator" content="Antora {{or antoraVersion site.antoraVersion}}">
{{/if}}
<link rel="stylesheet" href="{{uiRootPath}}/css/site.css">
{{#if env.COVEO_API_KEY}}
<link rel="stylesheet" href="https://static.cloud.coveo.com/searchui/v2.4382/css/CoveoFullSearch.min.css">
{{/if}}
{{> marketing-styles}}
<link rel="icon" href="{{uiRootPath}}/img/favicon.ico" type="image/x-icon">