{{#if page.component}}
<footer class="flex col doc-footer">
  {{#if env.GOOGLE_ANALYTICS_KEY}}
  <section class="feedback-section flex align-center col">
    <div class="feedback-first-row flex align-center">
      <div>
        <p>Was this article helpful?</p>
      </div>
      <div class="feedback-options flex">
        <button class="button feedback-yes" data-thanks="start" type="button">
          <img loading="lazy" src="{{@root.uiRootPath}}/img/icons/thumbs-up.png" alt="" height="12">
        </button>
        <button class="button feedback-no" data-thanks="start" type="button">
          <img loading="lazy" src="{{@root.uiRootPath}}/img/icons/thumbs-down.png" alt="" height="12">
        </button>
        <div class="feedback-ack flex hide">
          <img loading="lazy" src="{{@root.uiRootPath}}/img/icons/feedback-ack.svg" alt="" height="20">
          <p>You voted for:</p>
        </div>
        <button class="button give-feedback" data-thanks="start" type="button">
          Give feedback
        </button>
        {{!-- <span data-thanks="end">
          <img loading="lazy" src="{{@root.uiRootPath}}/img/icons/blue-heart.png" alt="" height="14">
          Thanks for your feedback!
        </span> --}}
      </div>
    </div>
    <div class="feedback-detailed-form hide">
      <form>
        <label for="fname">Share your feedback (required)</label><br>
        <input placeholder="Let us know what you liked, or how we can improve..." type="text" id="fname" name="fname"><br>
        <label for="lname">Please provide further details (optional)</label><br>
        <textarea placeholder="This is optional, but very helpful..." type="text" id="lname" name="lname" rows="4"></textarea>
        <span>Provide your name and email if youâ€™d like us to contact you:</span>
      </form>
    </div>
  </section>
  {{/if}}
  {{> github}}
  {{> mobile-survey }}
</footer>
{{/if}}