    <h1 class='page'>{{{page.title}}}</h1>
    {{{page.contents.cta}}}
  </header>
  <div class='panels'>
    <div class='panel'>
      {{{page.contents.latest-releases}}}
      {{{log (release-notes page.componentVersion.asciidoc.attributes.numberOfReleaseNotesItems)}}}
      {{!-- {{#each (release-notes)}}
        {{{log this}}} --}}
      {{!-- <a href="{{{relativize ./url}}}">{{{./doctitle}}}</a> --}}
      {{!-- {{/each}} --}}
    </div>
    <div class='panel'>
      {{{page.contents.popular-topics}}}
    </div>
  </div>
  {{{page.contents.the-road}}}
  <footer>
    {{{page.contents.more-resources}}}
  </footer>
</article>