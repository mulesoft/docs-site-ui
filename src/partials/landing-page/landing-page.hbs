<article class='doc'>
  <header>
    <h1 class='page'>{{{page.title}}}</h1>
    {{{page.contents.cta}}}
  </header>
  <div class='panels'>
    {{#if preview}}
    {{> contributor-signatures}}
    {{else}}
    {{> latest-releases}}
    {{/if}}
    {{> popular-topics}}
  </div>
  {{{page.contents.the-road}}}
  <footer>
    {{{page.contents.more-resources}}}
  </footer>
</article>